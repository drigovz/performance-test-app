{
  "root": true,
  "plugins": ["prettier"],

  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "parser": "babel-eslint"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "rules": {
    "no-console": "warn",
    "prettier/prettier": "error"
  }
}
